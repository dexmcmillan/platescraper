{
    "pagination": {
        "pages": 5,
        "records_per_page": 15,
        "next_page_button": "#searchResultsTable_next > a:nth-child(1)"
    },
    "click_through": "button",
    "unit": "#searchResultsTable > tbody > tr",
    "settings": {
        "speed": 3,
        "maximum_tries": 5,
        "puppeteerOptions": {
            "headless": false,
            "slowMo": 0,
            "defaultViewport": null
        }
    },
    "template": {
        "registration_number": "div.col-md-4:nth-child(1) > p:nth-child(2)",
        "date_added": "div.col-md-4:nth-child(4) > p:nth-child(2)",
        "govt_funding": "div.panel:nth-child(2) > div:nth-child(2) > div:nth-child(1) > div:nth-child(6) > ul:nth-child(2)",
        "lobbyists": {
            "type": "table",
            "row_selector": "#lobbyists > tbody:nth-child(2) > tr",
            "row_object_template": {
                "name": "td:nth-child(1)"
            }
        },
        "subject_matter": {
            "type": "table",
            "row_selector": "div.panel:nth-child(4) > div:nth-child(2) > div:nth-child(1) > table:nth-child(1) > tbody:nth-child(1) > tr:not(:first-child)",
            "row_object_template": {
                "description": "td:nth-child(1)",
                "subject_matter": "td:nth-child(2)",
                "categories": "td:nth-child(3)"
            }
        }
    },
    "urls": [
        "https://www.sasklobbyistregistry.ca/search-the-registry/"
    ]
}