{
    "template_name": "sask-lobbying",
    "pages": 5,
    "next_page_button": "#searchResultsTable_next > a:nth-child(1)",
    "unit": "#searchResultsTable > tbody > tr",
    "settings": {
        "speed": 3,
        "maximum_tries": 5,
        "save_as": "sask-lobbying.json",
        "puppeteerOptions": {
            "headless": false,
            "slowMo": 0,
            "defaultViewport": null
        }
    },
    "template": {
        "client": {
            "type": "single_field",
            "selector": "td:nth-child(1)"
        },
        "lobbyist": {
            "type": "single_field",
            "selector": "td:nth-child(2)"
        },
        "status": {
            "type": "single_field",
            "selector": "td:nth-child(3)"
        },
        "type": {
            "type": "single_field",
            "selector": "td:nth-child(4)"
        },
        "version": {
            "type": "single_field",
            "selector": "td:nth-child(5)"
        },
        "date_registered": {
            "type": "single_field",
            "selector": "td:nth-child(6)"
        },
        "business_activities": {
            "type": "single_field",
            "click_through": "tr.odd:nth-child(1) > td:nth-child(7) > button:nth-child(1)",
            "selector": "td:nth-child(6)"
        }
        
    },
    "urls": [
        "https://www.sasklobbyistregistry.ca/search-the-registry/"
    ]
}